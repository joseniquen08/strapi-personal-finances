{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email"
    },
    "name": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "accounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account.account",
      "mappedBy": "profile"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "profile"
    }
  }
}
