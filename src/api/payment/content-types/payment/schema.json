{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "amount": {
      "type": "decimal"
    },
    "paymentDate": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "paymentMethod": {
      "type": "string"
    },
    "notes": {
      "type": "text"
    },
    "isLate": {
      "type": "boolean"
    },
    "interestCharged": {
      "type": "decimal"
    },
    "account": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::account.account",
      "inversedBy": "payments"
    },
    "profile": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::profile.profile",
      "inversedBy": "payments"
    }
  }
